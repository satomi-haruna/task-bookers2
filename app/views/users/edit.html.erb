<h2>User info</h2>
<%= form_with model:@user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name,autofocus:true,id:"inputName" %> <!--placeholder:"名前" 入力前に自動的に入る文言-->
  <label for="inputImage">Image</label>
  <%= f.file_field :profile_image,placeholder:"Profile Image",accept:"image/*" %>
  <label for="inputIntroduction">Introduction</label>
  <!--自己紹介文、id:"introduction"とか入れるのかな？-->
  <%= f.text_area :introduction %>
  <%= f.submit "Update User" %>
<% end %>